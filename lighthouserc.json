{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "staticDistDir": "./build",
      "settings": {
        "preset": "desktop",
        "onlyCategories": ["accessibility"],
        "emulatedFormFactor": "desktop",
        "disableStorageReset": true
      }
    },
    "assert": {
      "assertions": {
        "categories:accessibility": ["error", { "minScore": 0.9 }]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./lhci-report"
    }
  }
}
